package Roles;

import javafx.scene.image.Image;

public class NoCharacter extends Character {
	public NoCharacter(int team, int initX, int initY) {
		super(initX, initY, team);
		System.out.println("test");
		level = Character.nocharacter;
		attackDistance = Character.nocharacter;
		moveDistance = Character.nocharacter;
		figure = new Image("pic/roles/left/infantryLeft.png");
		setCharacterViewAttribute();
	}
}
