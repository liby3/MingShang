package Roles;

import GameRule.Control;
import javafx.scene.image.Image;

public class Cavalryman extends Character {
	public Cavalryman(int team, int initX, int initY) {
		super(initX, initY, team);
		level = Character.cavalryman;
		attackDistance = Character.attackDistanceShort;
		moveDistance = Character.moveckDistanceLong;
		if (team == Control.LEFT_PLAYER) {
			figure = new Image("pic/roles/left/cavalrymanLeft.png");
		}
		else {
			
			figure = new Image("pic/roles/right/cavalrymanRight.png");
		}
	
	}{

}
