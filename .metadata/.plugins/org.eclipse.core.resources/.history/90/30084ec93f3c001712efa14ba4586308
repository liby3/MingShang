package Roles;

import GameRule.Control;
import javafx.scene.image.Image;

public class Archer extends Character {
	public Archer(int team, int initX, int initY, int level) {
		super(initX, initY, team, level);
		level = Character.archer;
		attackDistance = Character.attackDistanceLong;
		moveDistance = Character.moveDistanceShort;
		if (team == Control.LEFT_PLAYER) {
			figure = new Image("pic/roles/left/archerLeft.png");
		}
		else {
			
			figure = new Image("pic/roles/right/archerRight.png");
		}
	
	}
}
